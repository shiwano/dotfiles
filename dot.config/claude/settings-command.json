{
  "env": {
    "DISABLE_TELEMETRY": "1",
    "DISABLE_ERROR_REPORTING": "1"
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "defaultMode": "default",
    "allow": [
      "Bash(echo:*)",
      "Bash(cat:*)",
    ],
    "deny": [
      "Bash(sh:*)",
      "Bash(bash:*)",
      "Bash(zsh:*)",
      "Bash(sudo:*)",
      "Bash(wget:*)",
      "Bash(nc:*)",
      "Bash(dd:*)",
      "Bash(mkfs:*)",
      "Bash(chown -R:*)",
      "Bash(rm -rf:*)",
      "Bash(mv /*:*)",
      "Bash(cd /*:*)",
      "Bash(> /dev/sda:*)",
      "Bash(source:*)",
      "Bash(git branch -D:*)",
      "Bash(git branch -d:*)",
      "Bash(git push:*)",
      "Bash(gh:*)",
      "Bash(docker push:*)",
      "Bash(npm publish:*)",
      "Bash(brew:*)",
      "Bash(mise:*)",
      "Bash(deactivate:*)",
      "Read(.env*)",
      "Read(id_rsa)",
      "Read(id_ed25519)",
      "Read(**/*token*)",
      "Read(**/*key*)",
      "Read(**/secrets/**)"
    ]
  },
  "model": "claude-4-5-haiku-latest"
}
